{
  "build": {
    "beforeDevCommand": "npm run build",
    "beforeBuildCommand": "npm run build",
    "devPath": "../dist",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "cyber-sentinel-edu",
    "version": "2.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "open": true,
        "execute": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.cybersentinel.edu",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Cyber Sentinel Edu",
        "width": 1200,
        "height": 800,
        "decorations": true,
        "transparent": false
      }
    ]
  }
}
