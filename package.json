{
  "name": "word-hacker-404",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Word Hacker 404 - AI-Powered Word Game Application",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "echo 'Linting: Using TypeScript compiler for code validation' && npm run type-check",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "type-check": "tsc --noEmit",
    "quality-check": "node quality-check.js",
    "quality-check:verbose": "node quality-check.js --verbose",
    "deploy-gate": "node deploy-gate.js",
    "deploy": "npm run quality-check && npm run deploy-gate",
    "deploy:force": "node deploy-gate.js --force",
    "raw:bootstrap": "node ./scripts/raw/bootstrap.mjs",
    "raw:doctor": "node ./scripts/raw/doctor.mjs"
  },
