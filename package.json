{
  "name": "word-hacker-404",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Word Hacker 404 - AI-Powered Word Game Application",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "echo 'Linting: Using TypeScript compiler for code validation' && npm run type-check",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "quality-check": "node quality-check.js",
    "quality-check:verbose": "node quality-check.js --verbose",
    "deploy-gate": "node deploy-gate.js",
    "deploy": "npm run quality-check && npm run deploy-gate",
    "deploy:force": "node deploy-gate.js --force"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "audio-buffer-utils": "^5.1.2",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1",
    "meyda": "^5.6.3",
    "ml-matrix": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standardized-audio-context": "^25.3.77",
    "tone": "^15.1.22",
    "wavesurfer.js": "^7.11.0",
    "web-audio-beat-detector": "^8.2.31"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
