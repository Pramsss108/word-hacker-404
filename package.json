{
  "name": "word-hacker-404",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Word Hacker 404 - AI-Powered Word Game Application",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "echo 'Linting: Using TypeScript compiler for code validation' && npm run type-check",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "quality-check": "node quality-check.js",
    "quality-check:verbose": "node quality-check.js --verbose",
    "deploy-gate": "node deploy-gate.js",
    "deploy": "npm run quality-check && npm run deploy-gate",
    "deploy:force": "node deploy-gate.js --force",
    "raw:bootstrap": "node ./scripts/raw/bootstrap.mjs",
    "raw:doctor": "node ./scripts/raw/doctor.mjs"
  },
  "dependencies": {
    "@imgly/background-removal": "^1.7.0",
    "@react-spring/three": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.120.0",
    "@react-three/fiber": "^8.15.19",
    "@tensorflow/tfjs": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "audio-buffer-utils": "^5.1.2",
    "clsx": "^2.0.0",
    "comlink": "^4.4.2",
    "imagetracerjs": "^1.2.6",
    "jspdf": "^3.0.4",
    "libraw-wasm": "^1.1.2",
    "lucide-react": "^0.263.1",
    "meyda": "^5.6.3",
    "ml-matrix": "^6.12.1",
    "onnxruntime-web": "^1.23.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "standardized-audio-context": "^25.3.77",
    "svg2pdf.js": "^2.6.0",
    "three": "^0.163.0",
    "tone": "^15.1.22",
    "upscaler": "^1.0.0-beta.19",
    "utif": "^3.1.0",
    "wasm-vips": "^0.0.16",
    "wavesurfer.js": "^7.11.0",
    "web-audio-beat-detector": "^8.2.31"
  },
  "devDependencies": {
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.9.3",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^1.2.0"
  }
}
